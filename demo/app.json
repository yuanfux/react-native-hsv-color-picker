{
  "expo": {
    "name": "react-native-demo-app",
    "slug": "react-native-demo-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "packagerOpts": {
      "projectRoots": "",
      "config": "metro.config.js"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
